{
  "extends": "tslint:latest",
  "rules": {
    "no-reference": true,
    "no-console": [
      true,
      "log"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "variable-name": [
      true,
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ]
  },
  "jsRules": {
    "quotemark": [
      true,
      "single"
    ]
  }
}